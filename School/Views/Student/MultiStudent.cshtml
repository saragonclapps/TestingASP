@* Define in lowerCase *@
@model IEnumerable<Student>

@{
    ViewData["Title"] = "students information";
    Layout = "_Simple";
    ViewBag.Date = System.DateTime.Now;
}

<h1>@ViewData["Title"]</h1>
<p><i>@ViewBag.Date</i></p>

<table class="table table-dark table-hover">
    <thead>
    <tr>
        <td scope="col">
            <h3>NAME</h3>
        </td>
        <td scope="col">
            <h3>ID</h3>
        </td>
    </tr>
    </thead>
    <tbody>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.Name</td>
            <td>@student.UniqueId</td>
        </tr>
    }
    </tbody>
</table>
